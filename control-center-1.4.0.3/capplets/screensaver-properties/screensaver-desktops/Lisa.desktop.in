[Desktop Entry]
_Name=Lisa
_Comment=Draws animated full-loop lisajous figures
Exec=xscreensaver-command -activate
TryExec=lisa
Terminal=0
Type=Application

[Screensaver Data]
WindowIdCommand=-window-id
RootCommand=-root
Icon=lisa.png
_Author=Caleb Cullen
_ExtendedComment=Lisa draws animated full-loop lisajous figures.
Demo=lisa -delay 2

[Arg0]
_Name=Colors
Type=NRange
_LeftLabel=Colors:

Flag=-ncolors
ValType=I[1-256]=100
_Label=Number of Colors.

_Comment=Change the number of colors to use.

[Arg1]
_Name=Cycles
Type=LRange
Flag=-cycles
_LeftLabel=Faster

_RightLabel=Slower

ValType=I[1-10000]=8000
_Label=Speed of Motion.

_Comment=Change the delay in cycles.

[Arg2]
_Name=Count
Type=NRange
_LeftLabel=Count:

Flag=-count
ValType=I[1-5]=1
_Label=Number to use.

_Comment=Change the number to use.

[Arg3]
_Name=Size
Type=LRange
_LeftLabel=Smaller

_RightLabel=Larger

Flag=-size
ValType=I[50-1000]=100
_Label=Size of object.

_Comment=Change the size of object.

[Arg4]
_Name=Monochrome
Type=Check
Flag=-mono
Flag2=
_LeftLabel=Display screensaver in monochrome.

_Comment=Change to display in monochrome.

