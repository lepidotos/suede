[Desktop Entry]
_Name=Vines
_Comment=draws pseudo-fractal geometric patterns
Exec=xscreensaver-command -activate
TryExec=vines
Terminal=0
Type=Application

[Screensaver Data]
WindowIdCommand=-window-id
RootCommand=-root
Icon=vines.png
_Author=Tracy Camp
_ExtendedComment=The vines program is yet another geometric pattern generator, this one's claim to fame being a pseudo-fractal looking vine like pattern that creates nifty whirls and loops.
Demo=vines -delay 2

[Arg0]
_Name=Colors
Type=NRange
_LeftLabel=Colors:
Flag=-ncolors
ValType=I[1-256]=200
_Label=Number of Colors
_Comment=Change the number of colors to use.

[Arg1]
_Name=Monochrome
Type=Check
Flag=-mono
Flag2=
_LeftLabel=Display screensaver in monochrome.
_Comment=Change to display in monochrome.
