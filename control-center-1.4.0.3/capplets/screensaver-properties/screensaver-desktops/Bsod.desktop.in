[Desktop Entry]
_Name=Bsod
_Comment=Blue Screen of Death
Exec=xscreensaver-command -activate
TryExec=bsod
Terminal=0
Type=Application

[Screensaver Data]
WindowIdCommand=-window-id
RootCommand=-root
Icon=bsod.png
_Author=Jamie Zawinski <jwz@netscape.com>
_ExtendedComment=Bsod simulates crash screens from other operating systems.
DefaultFlags=-root
Demo=bsod -delay 2

[Arg0]
_Name=Delay
Type=LRange
Flag=-delay
_LeftLabel=Faster

_RightLabel=Slower

ValType=I[1-500000]=2
_Label=Speed of Motion.

_Comment=Change the delay.

[Arg1]
_Name=Monochrome
Type=Check
Flag=-mono
Flag2=
_LeftLabel=Display screensaver in monochrome.

_Comment=Change to display in monochrome.

