[Desktop Entry]
_Name=Greynetic
_Comment=Draws random color rectangles.
Exec=xscreensaver-command -activate
TryExec=greynetic
Terminal=0
Type=Application

[Screensaver Data]
WindowIdCommand=-window-id
RootCommand=-root
Icon=greynetic.png
_Author=Jamie Zawinski <jwz@jwz.org>
_ExtendedComment=Greynetic draws random rectangles.
Demo=greynetic -delay 2

[Arg0]
_Name=Delay
Type=LRange
Flag=-delay
_LeftLabel=Faster

_RightLabel=Slower

ValType=I[1-500000]=2
_Label=Speed of Motion.

_Comment=Change the delay in cycles.

[Arg1]
_Name=Monochrome
Type=Check
Flag=-mono
Flag2=
_LeftLabel=Display screensaver in monochrome.

_Comment=Change to display in monochrome.

