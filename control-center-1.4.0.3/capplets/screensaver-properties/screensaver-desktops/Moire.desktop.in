[Desktop Entry]
_Name=Moire
_Comment=Draw circular interference patterns
Exec=xscreensaver-command -activate
TryExec=moire
Terminal=0
Type=Application

[Screensaver Data]
WindowIdCommand=-window-id
RootCommand=-root
Icon=moire.png
_Author=Jamie  Zawinski <jwz@jwz.org>
_ExtendedComment=Moire draws circular interference patterns.
Demo=moire -delay 2


[Arg2]
_Name=Delay
Type=LRange
Flag=-delay
_LeftLabel=Faster

_RightLabel=Slower
ValType=I[1-500000]=2

_Label=Speed of Motion.

_Comment=Change the delay in cycles.

[Arg0]
_Name=Colors
Type=NRange
_LeftLabel=Colors:

Flag=-ncolors
ValType=I[1-264]=30
_Label=Number of Colors.

_Comment=Change the number of colors to use.

[Arg1]
_Name=Offset
Type=LRange
_LeftLabel=Smaller

_RightLabel=Larger

Flag=-offset
ValType=I[5-500]=500
_Label=Maximum radius increment

[Arg3]
_Name=Monochrome
Type=Check
Flag=-mono
Flag2=
_LeftLabel=Display screensaver in monochrome.

_Comment=Change to display in monochrome.


