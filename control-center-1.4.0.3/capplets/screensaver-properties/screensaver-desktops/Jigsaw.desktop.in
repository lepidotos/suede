[Desktop Entry]
_Name=Jigsaw
_Comment=Permutes the screen image like a jigsaw puzzle.
Exec=xscreensaver-command -activate
TryExec=jigsaw
Terminal=0
Type=Application

[Screensaver Data]
WindowIdCommand=-window-id
RootCommand=-root
Icon=jigsaw.png
_Author=Jamie Zawinski <jwz@jwz.org>
_ExtendedComment=Jigsaw takes an image of the screen, carves it up into jigsaw puzzle pieces, shuffles it, and solves it.
Demo=jigsaw -delay 2

[Arg0]
_Name=Delay
Type=LRange
Flag=-delay
_LeftLabel=Faster

_RightLabel=Slower

ValType=I[1-500000]=2
_Label=Speed of Motion.

_Comment=Change the delay in cycles.

