[Desktop Entry]
_Name=Munch
_Comment=Munching squares.
Exec=xscreensaver-command -activate
TryExec=munch
Terminal=0
Type=Application

[Screensaver Data]
WindowIdCommand=-window-id
RootCommand=-root
Icon=munch.png
_Author=Tim  Showalter  <tjs@andrew.cmu.edu>
_ExtendedComment=Munch munches squares until killed.
Demo=munch -delay 2

[Arg0]
_Name=Delay
Type=LRange
Flag=-delay
_LeftLabel=Faster

_RightLabel=Slower

ValType=I[1-500000]=2
_Label=Speed of Motion.

_Comment=Change the delay in cycles.

[Arg1]
_Name=XOR
Type=Check
Flag=-xor
Flag2=-no-xor
_LeftLabel=Use XOR drawing function.

_Comment=Change to use XOR drawing function.

[Arg2]
_Name=Shift
Type=Check
Flag=-shift
Flag2=-no-shift
_LeftLabel=Draw square at weird starting points.

_Comment=Change to draw square at weird starting points.

[Arg3]
_Name=Monochrome
Type=Check
Flag=-mono
Flag2=
_LeftLabel=Display screensaver in monochrome.

_Comment=Change to display in monochrome.

