[Desktop Entry]
_Name=Hypercube
_Comment=2D projection of a hypercube
Exec=xscreensaver-command -activate
TryExec=hypercube
Terminal=0
Type=Application

[Screensaver Data]
WindowIdCommand=-window-id
RootCommand=-root
Icon=hypercube.png
_Author=Jamie Zawinski <jwz@jwz.org>
_ExtendedComment=Hypercube displays a wireframe projection of a hypercube.
Demo=hypercube -delay 2

[Arg0]
_Name=Distance
Type=LRange
Flag=-observer-z
_LeftLabel=Closer

_RightLabel=Farther

ValType=I[1-15]=4
_Label=Distance from center of cube

_Comment=Change the distance from center of cube

[Arg1]
_Name=Delay
Type=LRange
Flag=-delay
_LeftLabel=Faster

_RightLabel=Slower

ValType=I[1-500000]=2
_Label=Speed of Motion.

_Comment=Change the delay in cycles.

[Arg2]
_Name=Monochrome
Type=Check
Flag=-mono
Flag2=
_LeftLabel=Display screensaver in monochrome.

_Comment=Change to display in monochrome.

