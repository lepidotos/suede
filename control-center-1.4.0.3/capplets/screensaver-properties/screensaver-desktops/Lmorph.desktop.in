[Desktop Entry]
_Name=Lmorph
_Comment=Morphing lines.
Exec=xscreensaver-command -activate
TryExec=lmorph
Terminal=0
Type=Application

[Screensaver Data]
WindowIdCommand=-window-id
RootCommand=-root
Icon=lmorph.png
_Author=Sverre  H.  Huseby <sverrehu@online.no> & Glenn T. Lines <gtl@si.sintef.no>
_ExtendedComment=Lmorph morphs between simple linedrawings using bilinear interpolation.
Demo=lmorph -delay 2

[Arg0]
_Name=Points
Type=NRange
_LeftLabel=Points:

Flag=-points
ValType=I[10-1000]=150
_Label=Number of points.

_Comment=Change the number of points to use.

[Arg1]
_Name=Steps
Type=LRange
_LeftLabel=Less

_RightLabel=More

Flag=-steps
ValType=I[100-500]=300
_Label=Number of interpolation steps.

_Comment=Change number of interpolation steps.

[Arg2]
_Name=Delay
Type=LRange
Flag=-delay
_LeftLabel=Faster

_RightLabel=Slower

ValType=I[1-500000]=2
_Label=Speed of Motion.

_Comment=Change the delay in cycles.

[Arg3]
_Name=Figtype
Type=Check
Flag=-figtype open
Flag2=-figtype closed

_LeftLabel=Open figures.

_Comment=Change from closed to open figures.

[Arg4]
_Name=Monochrome
Type=Check
Flag=-mono
Flag2=
_LeftLabel=Display screensaver in monochrome.

_Comment=Change to display in monochrome.

