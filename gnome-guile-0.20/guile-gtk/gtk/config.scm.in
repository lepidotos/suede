(define-public gtkconf-default-version "@GTK_VERSION@")
(define-public gtkconf-version #f)
(define-public gtkconf-gtk-2-0 @GTKCONF_GTK_2_0@)
(define-public gtkconf-guile-gtk-version "@VERSION@")
(define-public gtkconf-guilegtk-lib "@GUILEGTK_LIB@")
