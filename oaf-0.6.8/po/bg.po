#: liboaf/oaf-async-corba.c:186
msgid "No server corresponding to your query"
msgstr "Никой сървър не отговаря на заявката ви"

#: liboaf/oaf-fork-server.c:76
#, c-format
msgid "Failed to read from child process: %s\n"
msgstr "Невъзможно е четенето от дъщерен процес: %s\n"

#: liboaf/oaf-fork-server.c:175
msgid "Couldn't fork a new process"
msgstr "Невъзможно е отделянето на нов процес"

#: liboaf/oaf-fork-server.c:196
#, c-format
msgid "Child received signal %u (%s)"
msgstr "Дъщерният процес получи сигнал %u (%s)"

#: liboaf/oaf-fork-server.c:202
#, c-format
msgid "Unknown non-exit error (status is %u)"
msgstr "Непозната грешка, несвързана с изхода от програмата (състоянието е %u)"

#: liboaf/oaf-fork-server.c:257
msgid "Child process did not give an error message, unknown failure occurred"
msgstr "Дъщерния процес не сигнализира грешка, случи се непознат провал"

#: liboaf/oaf-fork-server.c:286
#, c-format
msgid "OAF failed to set process group of %s: %s\n"
msgstr "OAF не успя да установи групата на процесите на %s: %s\n"

#: liboaf/oaf-fork-server.c:297
#, c-format
msgid "Failed to execute %s: %d (%s)\n"
msgstr "Не успях да изпълня %s: %d (%s)\n"

#: liboaf/oaf-mainloop.c:221
